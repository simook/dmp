<%= render 'nav' %>
<div class="container-fluid">
  <div class="row-fluid">
    <div class="span3">
      <ul class="nav nav-list inner-well" data-bind="foreach:members">
        <li>
          <a href="#tab1" data-toggle="tab" data-bind="attr:{href:'#member'+characterID}">
            <span class="title" data-bind="text:name"></span>
            <i class="icon-chevron-right pull-right"></i>
          </a>
        </li>
      </ul>
    </div>
    <div class="span9 transparent-box">
      <div class="tab-content" data-bind="foreach:members">
        <div class="tab-pane padded" data-bind="attr:{id:'member'+characterID}">
          <div class="row-fluid">
            <div class="span12">
              <div class="box">
                <div class="tab-header">Member Info</div>
                <table class="table table-condensed box">
                  <tbody>
                    <tr>
                      <td><b>Name</b></td>
                      <td data-bind="text:name"></td>
                    </tr>
                    <tr>
                      <td><b>Location</b></td>
                      <td data-bind="text:location"></td>
                    </tr>
                    <tr>
                      <td><b>Last Seen</b></td>
                      <td data-bind="text:logoffDateTime"></td>
                    </tr>
                    <tr>
                      <td><b>Title</b></td>
                      <td data-bind="text:title"></td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<script type="text/javascript">
  dmp.corporation.init();
  <% if @corpadmin %>
  dmp.corporation.memberstracking();
  <% end %>
</script>