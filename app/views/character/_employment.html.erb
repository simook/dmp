<table class="table table-bordered table-condensed">
  <thead>
    <tr>
      <th>Corporation</th>
      <th>Start Date</th>
    </tr>
  </thead>
  <tbody>
    <% @info.employmentHistory.each do |e| %>
    <tr>
      <td><%= link_to id_to_name(e.corporationID), user_corporation_path(@user, e.corporationID) %></td>
      <td><%= e.startDate %></td>
    </tr>
    <% end %>
  </tbody>
</table>